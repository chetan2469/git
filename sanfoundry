    <span class="kw1">def</span> f1<span class="br0">&#40;</span>*args<span class="sy0">,</span> **kwargs<span class="br0">&#41;</span>:
           <span class="kw1">print</span><span class="br0">&#40;</span><span class="st0">&quot;Sanfoundry&quot;</span><span class="br0">&#41;</span>
           <span class="kw1">return</span> x<span class="br0">&#40;</span>*args<span class="sy0">,</span> **kwargs<span class="br0">&#41;</span>
